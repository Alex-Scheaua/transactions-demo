{
  "name": "transactions",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently \"yarn start-fe\" \"yarn start-be\"",
    "start-fe": "yarn --cwd ./fe dev",
    "start-be": "yarn --cwd ./be start",
    "setup": "yarn -i && concurrently \"yarn setup-fe\" \"yarn setup-be\"",
    "setup-fe": "yarn --cwd ./fe -i",
    "setup-be": "yarn --cwd ./be -i"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
